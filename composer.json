{
    "name": "dhruv125/ownwork",
    "description": "OwnWork - Minimal PHP Framework for an MVC Project",
    "type": "project",
    "require": {
        "php": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bundle\\": "bundle/",
            "App\\": "app/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "@php --server=localhost:8000 --docroot=public"
        ]
    },
    "authors": [
        {
            "name": "Sanatani-Dhruv"
        }
    ],
    "minimum-stability": "stable"
}
