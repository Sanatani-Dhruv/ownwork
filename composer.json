{
    "name": "dhruv125/ownwork",
    "description": "Creating own php framework to understand composer",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Bundle\\": "bundle/",
            "App\\": "app/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "start": [
            "cd public/ && php -S localhost:8000 && cd ../"
        ]
    },
    "authors": [
        {
            "name": "Sanatani-Dhruv"
        }
    ],
    "minimum-stability": "stable",
    "require": {}
}
